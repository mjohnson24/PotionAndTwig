/* HEADER ANNOUNCEMENT BAR */
.announcement-bar {
  background-color: {{ settings.header_bar_color_bg }};
}

.announcement-bar--link:hover {
  {% assign brightness = settings.header_bar_color_bg | color_brightness %}

  {% if brightness <= 192 %}
    {% assign lightenAmount = 255 | minus: brightness | divided_by: 255 | times: 16 %}
    background-color: {{ settings.header_bar_color_bg | color_lighten: lightenAmount }};
  {% else %}
    {% assign darkenAmount = 255 | divided_by: brightness | times: 8 %}
    background-color: {{ settings.header_bar_color_bg | color_darken: darkenAmount }};
  {% endif %}
}

.announcement-bar__message {
  color: {{ settings.header_bar_color_text }};
}
/* END HEADER ANNOUNCEMENT BAR */

/* HEADER COMMON */
.site-header{
  padding: 0;
  position: relative;
  top: 0;
  right: 0;
  left: 0;
  z-index: 999;
  transition: all 300ms;
  background-color: rgba($header_background_color, $header_background_color_opacity);
  background-size: $header_background_cover;
  background-repeat: $header_background_repeat;
  background-position: $header_background_pos_x $header_background_pos_y;
  border-top: $header_border_top_size solid $header_border_top_color;
  border-bottom: $header_border_bottom_size solid $header_border_bottom_color;
  .header-custom-content{
    padding: 13px 20px;
  }
  .top-header-content, .middle-header-content, .bottom-header-content{
    .border-left, &.border-left, .child-border-left > *{
      border-left: 1px solid;
    }
    .border-right, &.border-right, .child-border-right > *{
      border-right: 1px solid;
    }
    .border-top, &.border-top, .child-border-top > *{
      border-top: 1px solid;
    }
    .border-bottom, &.border-bottom, .child-border-bottom > *{
      border-bottom: 1px solid;
    }
  }
  .top-header-content{
    background-color: $header_top_background_color;
    color: $header_top_text_color;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-between;
    position: relative;
    display: flex;
    .top-header-left{
      display: flex;
      flex-wrap: wrap;
      align-items: center;
      &>*{
        float: left;
        &:first-child{
          border-left: none;
        }
      }
    }
    .top-header-right{
      display: flex;
      flex-wrap: wrap;
      align-items: center;
      &>*{
        float:left;
        &:first-child{
          border-left: none;
        }
      }
    }
    select{
      color: $header_top_text_color;
    }
    a{
      color: $header_top_link_color;
      &:hover{
        color: $header_top_link_hover_color;
        [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
          color: $header_top_icon_hover_color;
        }
      }
    }
    [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
      color: $header_top_icon_color;
      &:hover{
        color: $header_top_icon_hover_color;
      }
    }
    .header-search.show-on-click .block-title{
      &:after{
        color: $header_top_icon_color;
      }
      &:hover:after{
        color: $header_top_icon_hover_color;
      }
      &:before{
        background-color: $header_top_border_color;
      }
    }
    .header-cart .action-cart{
      &:before{
        color: $header_top_icon_color;
      }
      &:hover:before{
        color: $header_top_icon_hover_color;
      }
    }
    [class*=" border-"], &[class*=" border-"], [class*=" child-border-"] > *, [class^="child-border-"] > *, [class^="border-"], &[class^="border-"] {
      border-color: $header_top_border_color;
    }
    .header-account .account-title {
      &:before {
        color: $header_top_icon_color;
      }
      &:hover:before{
        color: $header_top_icon_hover_color;
      }
    }
  }
  .middle-header-content{
    background-color: $header_middle_background_color;
    background-repeat: $header_middle_background_repeat;
    background-position: $header_middle_background_pos_x $header_middle_background_pos_y;
    background-size: $header_middle_background_cover;
    color: $header_middle_text_color;
    select{
      color: $header_middle_text_color;
    }
    a{
      color: $header_middle_link_color;
      &:hover{
        color: $header_middle_link_hover_color;
        [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
          color: $header_middle_icon_hover_color;
        }
      }
    }
    [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
      color: $header_middle_icon_color;
      &:hover{
        color: $header_middle_icon_hover_color;
      }
    }
    .header-search.show-on-click .block-title{
      &:after{
        color: $header_middle_icon_color;
      }
      &:hover:after{
        color: $header_middle_icon_hover_color;
      }
      &:before{
        background-color: $header_middle_border_color;
      }
    }
    .header-cart .action-cart{
      &:before{
        color: $header_middle_icon_color;
      }
      &:hover:before{
        color: $header_middle_icon_hover_color;
      }
    }
    .header-account .account-title {
      &:before{
        color: $header_middle_icon_color;
      }
      &:hover:before{
        color: $header_middle_icon_hover_color;
      }
    }
    [class*=" border-"], &[class*=" border-"], [class*=" child-border-"] > *, [class^="child-border-"] > *, [class^="border-"], &[class^="border-"] {
      border-color: $header_middle_border_color;
    }
  }
  .bottom-header-content{
    background-color: $header_bottom_background_color;
    color: $header_bottom_text_color;
    select{
      color: $header_bottom_text_color;
    }
    a{
      color: $header_bottom_link_color;
      &:hover{
        color: $header_bottom_link_hover_color;
        [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
          color: $header_bottom_icon_hover_color;
        }
      }
    }
    [class*=" ti-"], [class^=ti-], [class*=" icon-"], [class^=icon-], [class*=" icon2-"], [class^=icon2-]{
      color: $header_bottom_icon_color;
      &:hover{
        color: $header_bottom_icon_hover_color;
      }
    }
    .header-search.show-on-click .block-title{
      &:after{
        color: $header_bottom_icon_color;
      }
      &:hover:after{
        color: $header_bottom_icon_hover_color;
      }
      &:before{
        background-color: $header_bottom_border_color;
      }
    }
    .header-cart .action-cart{
      &:before{
        color: $header_bottom_icon_color;
      }
      &:hover:before{
        color: $header_bottom_icon_hover_color;
      }
    }
    .header-account .account-title {
      &:before{
        color: $header_bottom_icon_color;
      }
      &:hover:before{
        color: $header_bottom_icon_hover_color;
      }
    }
    [class*=" border-"], &[class*=" border-"], [class*=" child-border-"] > *, [class^="child-border-"] > *, [class^="border-"], &[class^="border-"] {
      border-color: $header_bottom_border_color;
    }
  }
}

.toggle_menu_mobile{
  color: #333333;
  font-size: 18px;
  height: 100%;
  position: relative;
  cursor: pointer;
  span{
    position: relative;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    padding: 10px 0 10px 18px;
    display: block;
  }
}

.template-index{
  #header{
    &.is_absolute{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      &:not(.nav-up):not(.nav-down){
        background: transparent;
        .site-header{
          background: transparent;
          border-color: transparent;
          .top-header-content, .middle-header-content, .bottom-header-content{
            background: transparent;
            [class*=" border-"], &[class*=" border-"], [class*=" child-border-"] > *, [class^="child-border-"] > *, [class^="border-"], &[class^="border-"] {
              border-color: transparent;
            }
          }
        }
      }
    }
  }
}
/* END HEADER COMMON */

/* STICKY HEADER */
@media (min-width: 768px) {
  .sticky-header .site-header{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    transition: all 0.4s;
    transform: translateY(0);
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -o-transform: translateY(0);
  }
  #pd-sticky-mobile{
    display: none;
  }
  #header{
    &.nav-up{
      &.sticky-header .site-header {
        transform: translateY(-100%);
        -webkit-transform: translateY(-100%);
        -moz-transform: translateY(-100%);
        -o-transform: translateY(-100%);
      }
      .site-header .header-search.active .block-content {
        transform: scaleY(0.5);
        -webkit-transform: scaleY(0.5);
        -moz-transform: scaleY(0.5);
        -o-transform: scaleY(0.5);
        opacity: 0;
        visibility: hidden;
      }
    }
    {% if settings.header_background_color contains 'rgba(0,0,0,0)' %}
      &.nav-up, &.nav-down{
        &.is_absolute{
          .site-header{
            background: #fff;
          }
        }
      }
    {% endif %}
    &.nav-down {
      .site-header {
        box-shadow: 0 2px 38px 0 rgba(0,0,0,0.1);
        -moz-box-shadow: 0 2px 38px 0 rgba(0,0,0,0.1);
        -webkit-box-shadow: 0 2px 38px 0 rgba(0,0,0,0.1);
      }
    }
  }
}
@media (max-width: 767px) {
  .sticky-header-mobile .site-header{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    transition: all 0.4s;
    transform: translateY(0);
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -o-transform: translateY(0);
  }
  #pd-sticky{
    display: none;
  }
  #header{
    &.nav-up{
      &.sticky-header-mobile .site-header {
        transform: translateY(-100%);
        -webkit-transform: translateY(-100%);
        -moz-transform: translateY(-100%);
        -o-transform: translateY(-100%);
      }
    }
    &.nav-down {
      .site-header {
       box-shadow: 0 2px 38px 0 rgba(0,0,0,0.1);
      }
    }
    {% if settings.header_background_color contains 'rgba(0,0,0,0)' %}
      &.nav-up, &.nav-down{
        &.is_absolute{
          .site-header{
            background: #fff;
          }
        }
      }
    {% endif %}
  }
}
/* END STICKY HEADER */

/* HEADER CURRENCY */
.site-header{
  .switcher-currency{
    .currency-picker__wrapper{
      margin: 0;
      position: relative;
      &:after{
        content: "\e64b";
        font-family: "themify";
        font-size: 9px;
        margin-left: 6px;
        position: absolute;
        right: 20px;
        top: 50%;
        transform: translateY(-50%);
        -webkit-transform: translateY(-50%);
        -moz-transform: translateY(-50%);
        -o-transform: translateY(-50%);
        pointer-events: none;
      }
    }
    .currency-picker{
      border: 0;
      height: 47px;
      background: none;
      padding: 0 40px 0 17px;
      &:focus{
        border: 0;
        outline: none;
      }
    }
  }
}
/* END HEADER CURRENCY */

/* HEADER ACCOUNT */
.site-header{
  .header-account {
    float: right;
    position: relative;
    z-index: 3;
    .account-title {
      position: relative;
      display: block;
      padding: 15px 10px;
      strong {
        display: none;
      }
      &:before {
        content: "\e90d";
        font-family: 'arangi';
        font-size: 21px;
        cursor: pointer;
      }
    }
    &:hover{
      .my-account-link {
        .toplinks-content{
          opacity: 1;
          visibility: visible;
          transform: translate(50%,14px);
          -webkit-transform: translate(50%,14px);
          -moz-transform: translate(50%,14px);
          -o-transform: translate(50%,14px);
        }
      }
    }
    .my-account-link{
      position: relative;
      .toggle-action{
        padding: 13px 20px;
        display: block;
        &:after{
          content: "\e64b";
          font-family: "themify";
          font-size: 9px;
          margin-left: 6px;
        }
      }
      .toplinks-content{
        position: absolute;
        top: 100%;
        right: 50%;
        border: 1px solid #eeeeee;
        z-index: 2;
        opacity: 0;
        visibility: hidden;
        display: block;
        transition: all 300ms;
        transform: translate(50%,25px);
        -webkit-transform: translate(50%,25px);
        -moz-transform: translate(50%,25px);
        -o-transform: translate(50%,25px);
        box-shadow: 0 2px 38px 0 rgba(133,133,133, 0.25);
        background: #fff;
        padding: 30px 0;
        li{ 
          padding: 0 30px;
          &:last-child{
            a{
              border-bottom: 0;
            }
          }
          a{
            display: block;
            white-space: nowrap;
            padding: 14px 60px 14px 0;
            border-bottom: 1px dashed #eeeeee;
            font-family: {{ settings.typography_button_font.family }}
          }
        }
        &.active{
          opacity: 1;
          visibility: visible;
          transform: translateY(0);
          -webkit-transform: translateY(0);
          -moz-transform: translateY(0);
          -o-transform: translateY(0);
        }
      }
    }
  }
}
/* END HEADER ACCOUNT */

/* HEADER NEWSLETTER */
