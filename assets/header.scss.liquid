/* HEADER CART */
.site-header{
  .header-cart{
    position: relative;
    float: right;
    .action-cart{
      cursor: pointer;
      display: block;
      text-decoration: none;
      position: relative;
      padding: 15px 20px 15px 15px;
      &:before{
        font-size: 23px;
        content: "\e930";
        font-family: 'arangi';
        color: $minicart_icon_color;
        transition: color 0.3s;
      }
      .counter{
        height: 20px;
        line-height: 20px;
        border-radius: 30px;
        display: inline-block;
        margin: 0;
        padding: 0 3px;
        min-width: 20px;
        text-align: center;
        white-space: normal;
        font-size: 12px;
        font-weight: 400;
        vertical-align: top;
        color: $minicart_number_color;
        background-color: $minicart_number_background_color;
        position: absolute;
        right: 0;
        top: 15px;
        font-family: 'Raleway';
        z-index: 9;
      }
      .label-counter {
        display: none;
      }
    }
    .minicart-content{
      color: $minicart_text_color;
      background: $minicart_background_color;
      .header-cart-title{
        border-bottom: 1px solid $minicart_devide_border_color;
        padding-bottom: 10px;
      }
      h3{
        color: $minicart_heading_color;
        font-size: 15px;
        font-weight: 600;
        display: block;
        text-transform: uppercase;
      }
      .close_cart_slideout{
        color: $minicart_close_icon_color;
      }
      .empty{
        display: block;
        font-size: 14px;
        padding: 20px 0 5px;
        font-weight: 500;
        color: $minicart_text_color;
      }
      .minicart-dropdown-wrapper{
        &:after{
          content:'';
          display: table;
          width:100%;
        }
      }
      .minicart-items-wrapper{
        margin: 0 -30px;
        overflow-x: hidden;
        overflow-y: auto;
        padding: 0 30px;
        max-height: 263px;
        /* width */
        &::-webkit-scrollbar {
          width: 5px;
        }

        /* Track */
        &::-webkit-scrollbar-track {
          background: #eee; 
        }
        
        /* Handle */
        &::-webkit-scrollbar-thumb {
          background: #ddd; 
        }
      }
      .minicart-items{
        height: auto;
        .product-item{
          padding: 30px 0;
          &:after{
            content:'';
            display: table;
            width: 100%;
          }
          &:first-child{
            padding-top: 0;
          }
          &:not(:first-child) {
            border-top: 1px dashed $minicart_devide_border_color;
          }
          .product-item-photo{
            float: left;
            width: 70px;
            background-color: #faf9f7;
            img{
              display: block;
            }
          }
          .product-item-details{
            float: left;
            width: calc(100% - 70px);
            padding-left: 30px;
          }
          .product-item-name{
            font-size: 16px;
            font-weight: 500;
            display: block;
            font-family: 'Raleway';
          }
          .price{
            font-weight: 600;
            font-size: 15px;
            color: $color_price;
            margin-top: 10px;
          }
          .product-item-options{
            margin:0 0 10px;
            font-size: 12px;
            &:after{
              content: '';
              display: table;
              width: 100%;
            }
            dt, dd{
              float: left;
            }
            dt{
              clear: left;
              &:after{
                content: ':';
              }
            }
            dd{
              margin: 0 0 0 3px;
            }
          }
          .details-qty{
            display: flex;
            flex-wrap: wrap;
            width: 100%;
            margin-top: 10px;
            align-items: center;
            input{
              text-align: center;
              width: 50px;
              border-radius: 3px;
              padding: 0 3px;
              height: 32px;
            }
            .update-cart-item{
              font-size: 12px;
              box-sizing: border-box;
              cursor: pointer;
              box-shadow: none;
              border-radius: 3px;
              font-weight: 600;
              line-height: 1.6rem;
              padding: 0 10px;
              height: 32px;
              vertical-align: middle;
              margin-left: 10px;
              display: none;
            }
            .action.delete{
              margin-left: 15px;
              &:before{
                -webkit-font-smoothing: antialiased;
                font-size: 18px;
                line-height: 20px;
                content: "\e605";
                font-family: 'themify';
                vertical-align: middle;
                display: inline-block;
                font-weight: normal;
                overflow: hidden;
                speak: none;
                text-align: center;
              }
              span{
                display: none;
              }
            }
          }
        }
      }
      .summary{
        border-bottom: 1px solid $minicart_devide_border_color;
        border-top: 1px solid $minicart_devide_border_color;
        .subtotal{
          align-items: center;
          justify-content: space-between;
          padding: 20px 0 17px;
          display: flex;
          font-weight: 500;
          font-size: 15px;
          letter-spacing: 0.025em;
          .subtotal-title{
            text-transform: capitalize;
          }
          .subtotal-price {
            color: $color_price;
          }
        }
      }
      .actions{
        margin-top: 20px;
        a{
          width: 100%;
        }
        .checkout{
          margin-top: 10px;
        }
      }
    }
    .cart-dropdown{
      list-style: none none;
      border: 1px solid $minicart_devide_border_color;
      box-sizing: border-box;
      display: none;
      top: 100%;
      box-shadow: 0 2px 38px 0 rgba(133,133,133,0.25);
      width: 360px;
      max-width: 100vw;
      padding: 30px;
      right: 0;
      position: absolute;
      z-index: 1000;
      .product-item-name {
        display: flex !important;
        flex-wrap: nowrap;
        justify-content: space-between;
        align-items: flex-start;
        .action.delete{
          &:before{
            -webkit-font-smoothing: antialiased;
            font-size: 18px;
            line-height: 20px;
            content: "\e605";
            font-family: 'themify';
            vertical-align: middle;
            display: inline-block;
            font-weight: normal;
            overflow: hidden;
            speak: none;
            text-align: center;
          }
          span{
            display: none;
          }
        }
      }
      .actions{
        display: flex;
        justify-content: space-between;
        align-item: center;
        a{
          padding: 0 30px;
          text-transform: capitalize;
        }
        .checkout{
          margin-top: 0;
        }
      }
    }
    .cart-slideout{
      text-align: left;
      position: fixed;
      top: 0;
      background-color: #fff;
      right: 0;
      height: 100vh;
      width: 360px;
      max-width: calc(100vw - 40px);
      z-index: 100;
      padding: 20px;
      transition: all 400ms;
      transform: translateX(150%);
      -webkit-transform: translateX(150%);
      -moz-transform: translateX(150%);
      -o-transform: translateX(150%);
      display: block !important;
      .cart-slideout-inner{
        position: relative;
      }
      .minicart-items-wrapper{
        height: calc(100vh - 250px)!important;
        margin: 0 -30px;
        max-height: unset;
        .minicart-items{
          .product-item{
            &:first-child{
              padding-top: 0;
            }
          }
        }
      }
      .btn-minicart-close{
        position: absolute;
        top: 0;
        right: 0;
        border: none;
        padding: 0;
        background: none;
        cursor: pointer;
        font-size: 14px;
        font-weight: 500;
        letter-spacing: 0;
        box-shadow: none !important;
        &:after{
          content: "\e646";
          display: block;
          font-family: "themify";
          font-size: 20px;
          line-height: 20px;
        }
      }
    }
    &.toggle-cart-slide:after{
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100vh;
      content: "";
      z-index: 99;
      transition: all 400ms;
      opacity: 0;
      visibility: hidden;
      background-color: rgba(0, 0, 0, 0.6);
      transition-delay: 0.25s;
      -webkit-transition-delay: 0.25s;
      -moz-transition-delay: 0.25s;
      -o-transition-delay: 0.25s;
    }
    .cart-dropdown{
      display: block !important;
      opacity: 0;
      visibility: hidden;
      transition: all 300ms;
    }
    &.active{
      .cart-slideout{
        transform: translateX(0);
        -webkit-transform: translateX(0);
        -moz-transform: translateX(0);
        -o-transform: translateX(0);
      }
      &.toggle-cart-slide:after{
        opacity: 1;
        visibility: visible;
        transition-delay: 0s;
        -webkit-transition-delay: 0s;
        -moz-transition-delay: 0s;
        -o-transition-delay: 0s;
      }
      .cart-dropdown{
        opacity: 1;
        visibility: visible;
      }
    }
  }
}
